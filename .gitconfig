[user]
	name = James Vickery
	email = dev@jmsv.me
[alias]
	s = status
	a = add
	b = branch
	ba = branch --all
	aa = add -A
	co = checkout
	cob = checkout -b
	com = checkout master
	ci = commit
	cim = commit -m
	ciam = commit -a -m
	up = push
	down = pull
	po = push origin
	f = fetch
	fa = fetch --all
	l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	ch = cherry-pick
	sub = submodule
	subs = submodule
	subf = submodule foreach
	subinit = submodule update --init --recursive
	subm = submodule foreach git checkout master
	subpull = submodule foreach git pull
	diffc = diff --cached
	fresh = clean -d -x -f
	undocommit = reset HEAD~
	undoci = reset HEAD~
	uci = reset HEAD~
	fuck = commit --amend
[core]
	editor = vim
[push]
	default = simple
